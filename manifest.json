{
  "manifest_version": 2,
  "name": "Tabsdedup",
  "version": "1.0.0",
  "description": "Close duplicate Firefox tabs by exact URL in the current window or across all windows.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "Tabsdedup",
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabdedup@example.com",
      "strict_min_version": "91.0"
    }
  }
}
